@using Application.Domain.EF

<MudText Typo="Typo.h4">Person</MudText>
<hr />
<MudSimpleTable Style="overflow:auto; border:none">
    <tr>
        <MudTh Style="font-weight:bold">First Name</MudTh>
        <MudTd>@Person!.FirstName</MudTd>
    </tr>
    <tr>
        <MudTh Style="font-weight:bold">Last Name</MudTh>
        <MudTd>@Person!.LastName</MudTd>
    </tr>
</MudSimpleTable>




@code {
    [Parameter]
    public Person? Person { get; set; }
}
